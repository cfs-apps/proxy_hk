{ "app": {
   
   "title": "Housekeeping App",
   "version": "1.0.0",
   "supplier": "Open STEMware",
   "copyright": "bitValence",
   "url": "https://github.com/cfs-apps/proxy_hk",

   "description": [
      "This is Basecamp proxy for NASA's Housekeeping App. This repo augments",
      "the HK App repo so HK can be integrated with Basecamp. The 'cfs' and",
      "'requires' JSON objects are populated in hk.json"
   ],

   "proxy": {
      "url": "https://github.com/nasa/hk",
      "branch-tag": "equuleus-rc1",
      "update-files": [
         "hk.json                      >> hk.json",
         "CMakeLists.txt               >> CMakeLists.txt",
         "eds/hk.xml                   >> eds/hk.xml",
         "eds/hk_config.xml            >> eds/hk_config.xml",
         "fsw/inc/hk_msgids.h          >> fsw/inc/hk_msgids.h",
         "fsw/inc/hk_extern_typedefs.h >> fsw/inc/hk_extern_typedefs.h" 
      ]
   },
      
   "cfs": {
      "cfe-type": "CFE_PROXY"
   },
   
   "requires": []
   
}}
